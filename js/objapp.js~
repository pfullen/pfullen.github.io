
// Guess Game Object
function GuessGame (currentGuess, lastGuess, randNum){

  this.currentGuess = currentGuess;
  this.lastGuess = lastGuess;
  this.randNum = randNum;
  
  var currentResult = Math.abs(this.currentGuess - this.randNum);
  var lastResult = Math.abs(this.lastGuess - this.randNum);  

// Get result method  
  this.result = function (){
    if ( currentResult == 0 ) {
    	return 0;
     } else if (last reslut == null) {
            if (currentResult > 10) {
				return 10;            
            } else {
				return -10;
			}    	     	    	
    } else if (currentResult < lastResult ) {
		
		return 1;	  
	  
	  } else {
		return -1;	  
	  }

}

// Get Random number method
this.GetRanNum = function {
	var randomNum = Math.floor((Math.random()*100)+1);
  	return randomNum;

}



};
// End of Guess Game Object




